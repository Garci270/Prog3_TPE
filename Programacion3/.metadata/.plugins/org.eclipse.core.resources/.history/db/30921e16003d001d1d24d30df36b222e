package JuegoPersonajes;

public class Personaje {
	private String nombre;
	private String nombre_heroe;
	private int vision_nocturna;
	private int velocidad;
	private int fuerza;
	private int peso;
	private int altura;
	private int edad;

	public Personaje(String nombre, String nombre_heroe, int vis, int vel, int fuer, int peso,int alt,int edad) {
		this.nombre = nombre;
		this.nombre_heroe = nombre_heroe;
		this.vision_nocturna = vis;
		this.velocidad = vel;
		this.fuerza = fuer;
		this.peso = peso;
		this.altura = alt;
		this.edad = edad;
	}
	
